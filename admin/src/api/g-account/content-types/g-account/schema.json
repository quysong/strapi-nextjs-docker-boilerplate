{
  "kind": "collectionType",
  "collectionName": "g_accounts",
  "info": {
    "singularName": "g-account",
    "pluralName": "g-accounts",
    "displayName": "g_account"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
